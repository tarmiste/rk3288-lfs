diff --git a/fs/aufs/dentry.c b/fs/aufs/dentry.c
new file mode 100644
index 000000000000..230db04b1aee
--- /dev/null
+++ b/fs/aufs/dentry.c
@@ -0,0 +1,1152 @@
+/*
